<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.models.CustomersEntity" table="Customers" schema="JAVACAFE">
        <property name="name">
            <column name="name" sql-type="varchar(80)" length="80" not-null="true"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(80)" length="80" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>